type Vector
{
	var x;
	var y;
}

func (*) (Vector a, b) -> (Vector c)
{
	c.x = a.x*b;
	c.y = a.y*b;
}
func (/) (Vector a, b) -> (Vector c)
{
	c.x = a.x/b;
	c.y = a.y/b;
}

func (*) (Vector a, Vector b) = (a.x*b.x + a.y*b.y);
func (+) (Vector a, Vector b) -> (Vector c)
{
	c.x = a.x + b.x;
	c.y = a.y + b.y;
}
func (-) (Vector a, Vector b) -> (Vector c)
{
	c.x = a.x - b.x;
	c.y = a.y - b.y;
}