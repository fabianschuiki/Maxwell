{
	"interfaces": {
		"range": [
			"\\Source\\Range range",
			"\\Source\\Range humanRange"],
		"binding": [
			"Expr bindingTarget"],
		"type": [
			"Type type"],
		"root_code": [
			"string indepDeclCode",
			"string depDeclCode",
			"string indepDefCode",
			"string depDefCode"],
		"expr_code": [
			"string exprCode",
			"string stmtsCode"]
	},

	"entities": {
		"IdentifierExpr": {
			"main": ["@range", "string name"],
			"binding": ["@binding"],
			"type": ["@type", "string someText"],
			"code": ["@expr_code"]},

		"VariableDefinitionExpr": {
			"implements": "Expr",
			"main": ["@range", "string name", "TypeExpr typeExpr", "Expr initialExpr"],
			"type": ["@type"],
			"code": ["@expr_code"]},

		"FunctionDefinition": {
			"root": true,
			"tree": ["*IdentifierExpr ident"],
			"main": ["@range", "string name", "*IdentifierExpr inner"],
			"type": ["@type"],
			"code": ["@root_code"]}
	}
}