//Square roots.
func sqrt (int a) -> (int r) {}
func sqrt (float a) -> (float r) {}

//A mathematical vector.
type Vector
{
	var x;
	var y;
	
	func (*) (Vector other) -> (result) { result = this.x*other.x + this.y*other.y; }
}

//A mathematical norm.
func norm (a) -> (v) { sqrt(a*a); }

//A straight line between two points.
type Line
{
	var Vector s;
	var Vector e;
	
	func direction { return this.e - this.s; }
	func length { return norm(this.direction); }
}

