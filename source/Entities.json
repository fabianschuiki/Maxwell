{
	"interfaces": {
		"range": [
			"\\Source\\Range range",
			"\\Source\\Range humanRange"],
		"binding": [
			"\\RepositoryObjectReference bindingTarget"],
		"type": [
			"Type type"],
		"root_code": [
			"string indepDeclCode",
			"string depDeclCode",
			"string indepDefCode",
			"string depDefCode"],
		"expr_code": [
			"string exprCode",
			"string stmtsCode"],
		"graph": [
			"\\RepositoryObjectReference graphPrev"
			]
	},

	"entities": {
		"IdentifierExpr": {
			"extends": "Expr",
			"main": ["@range", "string name"],
			"graph": ["@graph"],
			"binding": ["@binding"],
			"type": ["@type", "string someText"],
			"code": ["@expr_code"]},

		"VariableDefinitionExpr": {
			"implements": "Expr",
			"main": ["@range", "string name", "TypeExpr typeExpr", "Expr initialExpr"],
			"type": ["@type"],
			"code": ["@expr_code"]},

		"FunctionDefinition": {
			"root": true,
			"tree": ["*FunctionArgumentTuple inputs", "*FunctionArgumentTuple outputs", "*FunctionBody body"],
			"main": ["@range", "string name"],
			"graph": ["@graph"],
			"type": ["@type"],
			"code": ["@root_code"],
			"binding": []},

		"FunctionArgumentTuple": {
			"tree": ["\\RepositoryObjectArray arguments"],
			"graph": ["@graph"]
		},

		"FunctionArgument": {
			"tree": ["TypeExpr typeExpr"],
			"main": ["string name"],
			"graph": ["@graph"]
			},

		"FunctionBody": {
			"tree": ["\\RepositoryObjectArray stmts"]
		},

		"ExprStmt": {
			"tree": ["Expr expr"],
			"graph": ["@graph"]
		},

		"AssignmentExpr": {
			"extends": "Expr",
			"tree": ["Expr lhs", "Expr rhs"],
			"graph": ["@graph"]
		},

		"BinaryOperatorExpr": {
			"extends": "Expr",
			"tree": ["Expr lhs", "Expr rhs"],
			"main": ["string operator"],
			"graph": ["@graph"]
		},

		"ConstantExpr": {
			"extends": "Expr",
			"main": ["string value"],
			"graph": ["@graph"]
		},

		"TypeExpr": {
			"tree": ["Expr expr"],
			"graph": ["@graph"]
			}
	}
}